{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang build active file",
            "command": "/usr/bin/clang",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${fileDirname}/priority_queue.c",
                "${fileDirname}/queue.c",
                "${fileDirname}/binary_tree.c",
				"${fileDirname}/data_structure.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "compiler: /usr/bin/clang"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/clang",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${fileDirname}/priority_queue.c",
                "${fileDirname}/queue.c",
				"${fileDirname}/binary_tree.c",
				"${fileDirname}/data_structures.c",
                "-o",
                "${workspaceFolder}/build/${fileBasenameNoExtension}",
            ],
            "options": {
                "cwd": "${workspaceFolder}/source"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
				"isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}